{
  "name": "@getdinghy/design-system",
  "version": "0.0.1",
  "private": true,
  "main": "dist/server",
  "scripts": {
    "start": "node .",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "danger": "danger pr",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint src,private webpack.config.js --cache --cache-location=.cache/eslint --ext .js,.jsx,.json",
    "lint:md": "remark .",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "build-storybook": "build-storybook -c private/storybook",
    "deploy-storybook": "bash private/scripts/publish_storybook.sh",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack",
    "bootstrap": "lerna bootstrap --concurrency 1 --npm-client=\"yarn\" --hoist && node private/scripts/hoist_internals.js",
    "build-packs": "lerna exec --scope '@getdinghy/*' --parallel -- private/scripts/build_packs.sh packs",
    "prepublish": "node private/scripts/prepublish_components.js",
    "publish-components": "lerna publish"
  },
  "jest": {
    "moduleDirectories": [
      "app/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    }
  },
  "storybook-deployer": {
    "gitUsername": "gipsy",
    "gitEmail": "ter4444@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js -- --fix",
      "git add"
    ],
    "*.json": [
      "npm run lint:js -- --fix",
      "git add"
    ],
    "*.md": [
      "npm run lint:md -- -o",
      "git add"
    ]
  },
  "pr-log": {
    "skipLabels": [
      "cleanup"
    ],
    "validLabels": {
      "breaking": "Breaking Changes",
      "feature": "Features",
      "bug": "Bug Fixes",
      "documentation": "Documentation",
      "maintenance": "Maintenance",
      "greenkeeper": "Dependency Upgrades",
      "other": "Other"
    }
  },
  "devDependencies": {
    "@buildit/storybook-addon-blabbr": "^1.1.0",
    "@buildit/storybook-addon-versions": "^0.4.3",
    "@storybook/addon-actions": "^3.1.9",
    "@storybook/addon-centered": "^3.2.0",
    "@storybook/addon-chapters": "^0.6.2",
    "@storybook/addon-comments": "^3.1.9",
    "@storybook/addon-info": "^3.1.9",
    "@storybook/addon-knobs": "3.2.0",
    "@storybook/addon-notes": "^3.2.0",
    "@storybook/addon-options": "^3.1.9",
    "@storybook/react": "^3.1.9",
    "@storybook/storybook-deployer": "^2.0.0",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-docgen": "^1.6.0",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.0.0",
